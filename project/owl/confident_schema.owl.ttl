        linkml:SlotDefinition ;
    rdfs:label "context_info" ;
    dc:title "Context Information" ;
    rdfs:range confident:Context ;
    skos:definition "A property to provide extra information, such as call of papers summary,  event or event series description." .

confident:former_name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "former_name" ;
    dc:title "Former Name" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf sdo:name ;
    skos:definition "The former official name of an academic event or series. Usually this will only be needed in case an academic event series has undergone a name change." .

confident:has_acronym a owl:ObjectProperty,
        linkml:SlotDefinition ;
