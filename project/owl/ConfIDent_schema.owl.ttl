aeon:0000040 a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "academic_fields" ;
    rdfs:domain confident:PlannedProcess ;
    rdfs:range confident:AcademicField ;
    skos:definition "A property to describe the scientific subject(s) of a planned process, according to some controlled vocabulary or thesaurus. If this is used, its subproperties [value] and [schema_name] are mandatory." ;
    skos:exactMatch aeon:0000040 .

