    skos:exactMatch wikidata:Q43649390 .

confident:context_info a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "context_info" ;
    rdfs:domain confident:PlannedProcess ;
    rdfs:range confident:Context ;
