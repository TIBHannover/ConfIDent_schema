            owl:allValuesFrom confident:Sponsor ;
            owl:onProperty confident:sponsors ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass confident:Context ;
            owl:onProperty confident:context_info ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass confident:AcademicEvent ;
            owl:onProperty confident:series_of ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:DigitalObjectId ;
            owl:onProperty iao:0000235 ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:Metric ;
            owl:onProperty confident:metrics ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty confident:id ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:AcademicField ;
            owl:onProperty aeon:0000040 ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:Publication ;
            owl:onProperty confident:publications ],
        [ a owl:Restriction ;
            owl:onClass confident:ProcessName ;
            owl:onProperty iao:0000235 ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:DblpId ;
            owl:onProperty iao:0000235 ],
        [ a owl:Restriction ;
            owl:allValuesFrom confident:WikiCfpId ;
            owl:onProperty iao:0000235 ],
        confident:PlannedProcess ;
    skos:closeMatch sdo:EventSeries ;
    skos:definition "An academic event series describes the set of academic events which take place on a regular basis and thus have an established common identity. This identity is constituted, for example, through institutional continuity in the hosting of a series (e.g. by a specialised society), thematic focuses and/or a common label under which a series is defined (particularly name and acronym). Nevertheless, it is possible that each of these criteria may change over time." ;
    skos:exactMatch aeon:0000002 .

